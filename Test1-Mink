local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Shy Hub - Mink Piece", "Ocean")

--AutoFarm
local Autofarm = Window:NewTab("Autofarm")
local AutofarmSection = Autofarm:NewSection("Autofarm")


AutofarmSection:NewButton("Meele Autofarm", "It farms Meele, go near the Immortal Monke ", function()
    -- Meele Autofarm 

while true do 
    
    wait(0)
    
    local args = {
        [1] = workspace:FindFirstChild("Immortal Monke"),
        [2] = 1,
        [3] = "Melee"
    }
    
    game:GetService("ReplicatedStorage").Remotes.DmgEvent:FireServer(unpack(args))
    
    
    local args = {
        [1] = workspace:FindFirstChild("Immortal Monke"),
        [2] = 2,
        [3] = "Melee"
    }
    
    game:GetService("ReplicatedStorage").Remotes.DmgEvent:FireServer(unpack(args))
    
    
    local args = {
        [1] = workspace:FindFirstChild("Immortal Monke"),
        [2] = 3,
        [3] = "Melee"
    }
    
    game:GetService("ReplicatedStorage").Remotes.DmgEvent:FireServer(unpack(args))
    
    
    local args = {
        [1] = workspace:FindFirstChild("Immortal Monke"),
        [2] = 4,
        [3] = "Melee"
    }
    
    game:GetService("ReplicatedStorage").Remotes.DmgEvent:FireServer(unpack(args))
    end 
    
end)










AutofarmSection:NewButton("Meele TP", "It teleports you to the Immortal Monke! ", function()

    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(3489.53711, 4.00007248, 157.354691, 0.0613911971, -7.77280889e-08, 0.998113811, 8.60582983e-09, 1, 7.73456605e-08, -0.998113811, 3.84125443e-09, 0.0613911971)

end)










